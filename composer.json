{
    "name": "andonovn/laravel-bets-api",
    "description": "Consume the betsapi.com service",
    "keywords": ["laravel", "betsapi.com"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolay Andonov",
            "email": "nikolay.andonov@windowslive.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "~5.4",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Andonovn\\LaravelBetsApi\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Andonovn\\LaravelBetsApi\\ServiceProvider"
            ]
        }
    }
}
